{
  "name": "mertester",
  "version": "0.0.1",
  "description": "",
  "author": "The Merlion Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write --loglevel warn \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/cli",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typechain": "./scripts/typechain.sh",
    "postinstall": "yarn typechain",
    "hardhat:compile": "npx hardhat compile",
    "hardhat:clean": "npx hardhat clean",
    "hardhat:test": "npx hardhat test"
  },
